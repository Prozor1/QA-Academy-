const {Builder, By} = require('selenium-webdriver');
const chrome = require('selenium-webdriver/chrome');

(async function locatorsDemo() {
  let driver = await new Builder().forBrowser('chrome').setChromeOptions(new chrome.Options().excludeSwitches(['enable-logging'])).build();
  try {
    await driver.get('https://www.w3schools.com/html/html_forms.asp');
    console.log('Ð¡Ñ‚Ñ€Ð°Ð½Ð¸Ñ†Ð° Ñ Ñ„Ð¾Ñ€Ð¼Ð¾Ð¹ Ð¾Ñ‚ÐºÑ€Ñ‹Ñ‚Ð°.');

    let firstName = await driver.findElement(By.id('fname'));
    await firstName.clear();
    await firstName.sendKeys('John');

    let lastName = await driver.findElement(By.name('lname'));
    await lastName.clear();
    await lastName.sendKeys('Doe');

    console.log('Ð”Ð°Ð½Ð½Ñ‹Ðµ Ð²Ð²ÐµÐ´ÐµÐ½Ñ‹ Ð² Ñ„Ð¾Ñ€Ð¼Ñƒ.');
    await new Promise(r => setTimeout(r, 5000));
  } finally {
    await driver.quit();
  }
})();

ðŸ“„ test4_click_button.js
const {Builder, By} = require('selenium-webdriver');
const chrome = require('selenium-webdriver/chrome');

(async function clickButtonDemo() {
  let driver = await new Builder().forBrowser('chrome').setChromeOptions(new chrome.Options().excludeSwitches(['enable-logging'])).build();
  try {
    await driver.get('https://www.w3schools.com/jsref/tryit.asp?filename=tryjsref_alert');
    await driver.switchTo().frame('iframeResult');
    let button = await driver.findElement(By.tagName('button'));
    await button.click();
    console.log('ÐšÐ½Ð¾Ð¿ÐºÐ° Ð½Ð°Ð¶Ð°Ñ‚Ð°!');
    await new Promise(r => setTimeout(r, 5000));
  } finally {
    await driver.quit();
  }
})();

