const {Builder, By, Key} = require('selenium-webdriver');
const chrome = require('selenium-webdriver/chrome');

(async function amazonSearch() {
  let driver = await new Builder().forBrowser('chrome').setChromeOptions(new chrome.Options().excludeSwitches(['enable-logging'])).build();
  try {
    await driver.get('https://www.amazon.com');
    let searchBox = await driver.findElement(By.id('twotabsearchtextbox'));
    await searchBox.sendKeys('laptop', Key.RETURN);
    console.log('Ð’Ñ‹Ð¿Ð¾Ð»Ð½ÐµÐ½ Ð¿Ð¾Ð¸ÑÐº Ð¿Ð¾ ÑÐ»Ð¾Ð²Ñƒ "laptop"');
    await new Promise(r => setTimeout(r, 5000));
  } finally {
    await driver.quit();
  }
})();

